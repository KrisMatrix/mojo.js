{
  "name": "@mojojs/mojo",
  "version": "0.0.4",
  "description": "Real-time web framework",
  "keywords": [
    "web",
    "framework"
  ],
  "author": "Sebastian Riedel <kraih@mojolicious.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojolicious/mojo.js.git"
  },
  "bugs": {
    "url": "https://github.com/mojolicious/mojo.js/issues"
  },
  "homepage": "https://mojojs.org",
  "scripts": {
    "build": "npx tsc",
    "build:lint": "npm run build && eslint \"src/**/*.js\" \"src/*.js\" \"test/*.js\" \"src/**/*.ts\" \"src/*.ts\"",
    "build:test": "npm run build && npm test",
    "build:watch": "npx tsc --watch",
    "coverage": "tap --node-arg='--no-warnings' --node-arg='--experimental-loader' --node-arg='@istanbuljs/esm-loader-hook' test/*.js",
    "coverage-html": "tap --node-arg='--no-warnings' --node-arg='--experimental-loader' --node-arg='@istanbuljs/esm-loader-hook' --coverage-report=html test/*.js",
    "lint": "eslint \"src/**/*.js\" \"src/*.js\" \"test/*.js\" \"src/**/*.ts\" \"src/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "tap --no-coverage test/*.js"
  },
  "exports": "./lib/mojo.js",
  "types": "./lib/mojo.d.ts",
  "type": "module",
  "files": [
    "bin/",
    "lib/",
    "vendor/"
  ],
  "bin": {
    "mojo": "./bin/mojo.js"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^=0.1.2",
    "@types/node": "^15.12.2",
    "@types/stack-utils": "^2.0.0",
    "@types/tap": "^15.0.3",
    "@types/tough-cookie": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "typescript": "^=4.3.2"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "ajv": "^8.5.0",
    "busboy": "^0.3.1",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.1",
    "ejs": "^3.1.6",
    "escape-string-regexp": "^5.0.0",
    "form-data": "^4.0.0",
    "lru-cache": "^6.0.0",
    "mime-types": "^2.1.30",
    "nopt": "^5.0.0",
    "stack-utils": "^2.0.3",
    "tap": "^15.0.6",
    "tough-cookie": "^4.0.0",
    "ws": "^7.4.5"
  }
}
